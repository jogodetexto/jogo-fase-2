	funcao savejoao()
	{
		
		escreva("(Luigu)-Joao, calma, sera que realmente o melhor é fugir e nao lutar??\n")
		u.aguarde(5000)
		escreva("(Joao Antonio)-talvez seja mas sera que realmente vale a pena?\n")
		u.aguarde(5000)
